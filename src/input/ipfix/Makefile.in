CC      = @CC@
CFLAGS  = @CFLAGS@
LIBS    = @LIBS@
INCLUDE = "-I../../../"

all: ipfix_input.so

ipfix_input.so: ipfix_input.o
	$(CC) $(CFLAGS) $(LIBS) -shared -Wl,-soname,ipfix_input.so.1 ipfix_input.o -o $@ -lc
	
ipfix_input.o: ipfix_file.c
	$(CC) $(CFLAGS) $(INCLUDE) -fPIC -c -o $@ $<

clean:
	-rm ipfix_input.so ipfix_input.o


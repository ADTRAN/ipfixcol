<?xml version="1.0" encoding="UTF-8"?>
<ipfixcol xmlns="urn:cesnet:params:xml:ns:yang:ipfixcol-internals">

	<!--## List of supported collectors
		## One of these elements is expected in <collectingProcess>
		##   element in startup.xml 
	 -->
	<supportedCollectors>
		<name>sctpCollector</name>
		<name>udpCollector</name>
		<name>tcpCollector</name>
		<name>fileReader</name>
	</supportedCollectors>

	<!--## Input plugin internal configuration -->
	<inputPlugin>
		<!--## Name must match one of the <supportedCollectors> -->
		<name>udpCollector</name>
		<!--## Path of the plugin module -->
		<file>src/input/udp/ipfixcol_udp_input.so</file>
	</inputPlugin>
	<inputPlugin>
		<name>tcpCollector</name>
		<file>src/input/tcp/ipfixcol_tcp_input.so</file>
	</inputPlugin>
	<inputPlugin>
		<name>fileReader</name>
		<file>src/input/ipfix/ipfixcol_ipfix_input.so</file>
	</inputPlugin>
	<inputPlugin>
		<name>sctpCollector</name>
		<file>src/input/sctp/ipfixcol_sctp_input.so</file>
	</inputPlugin>

	<!--## Storage plugin internal configuration -->
	<storagePlugin>
		<!--## Definition of file format
		    ## The element <fileFormat> in startup.xml must
		    ##   match this element -->
		<fileFormat>ipfix</fileFormat>
		<!--## Path of the storage module  -->
		<file>src/storage/ipfix/ipfixcol_ipfix_output.so</file>
	</storagePlugin>

	<!-- fastbit storage plugin
	<storagePlugin>
		<fileFormat>fastbit</fileFormat>
		<file>src/storage/fastbit/fastbit_output.so</file>
	</storagePlugin> -->

	<!-- postgress storage plugin
	<storagePlugin>
        <fileFormat>postgres</fileFormat>
        <file>src/storage/postgres/postgres_output.so</file>
	</storagePlugin> -->
</ipfixcol>
